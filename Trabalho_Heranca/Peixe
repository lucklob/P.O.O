	public void locomover() 
	{
		System.out.println("Peixe est치 nadando!");	
	}

	@Override
	public void alimentar() 
	{
		System.out.println("Peixe est치 comendo");
		
	}

	@Override
	public void emitirSom() 
	{
		System.out.println("Peixe est치 emitindo som!");
		
	}
	
	public void soltarBolhas()
	{
		System.out.println("Peixe est치 soltando bolhas!");
	}

}
